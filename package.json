{
  "name": "streaming-tools",
  "version": "1.0.0",
  "description": "some scripts for streaming",
  "main": "main.js",
  "scripts": {
    "give-snoreToast-permissions": "chmod +x node_modules/node-notifier/vendor/snoreToast/snoretoast-x64.exe; chmod +x node_modules/node-notifier/vendor/snoreToast/snoretoast-x86.exe",
    "postinstall": "npm run give-snoreToast-permissions",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "prestart": "npm run build",
    "start": "node main.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon main.js",
    "dev:client": "cd client && BROWSER=none npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.0.0",
    "@tensorflow/tfjs-node": "^3.0.0",
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "color-name-list": "^7.14.0",
    "color-space": "^1.16.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "get-image-colors": "^4.0.0",
    "googleapis": "^71.0.0",
    "human-to-cron": "^0.3.1",
    "image-data-uri": "^2.0.1",
    "image-url": "^1.1.0",
    "jimp": "^0.16.1",
    "memory-cache": "^0.2.0",
    "ngrok": "^3.4.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.0.3",
    "obs-websocket-js": "^4.0.2",
    "opencv4nodejs": "^5.6.0",
    "pureimage": "^0.3.0",
    "qs": "^6.9.4",
    "replaceall": "^0.1.6",
    "socket.io": "^2.3.0",
    "tesjs": "^0.4.0",
    "tmi.js": "^1.7.1",
    "uuid": "^8.3.1",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "chalk": "4.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.1",
    "nodemon": "2.0.7",
    "prettier": "2.2.1"
  }
}
