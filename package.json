{
  "name": "streaming-tools",
  "version": "1.0.0",
  "description": "some scripts for streaming",
  "main": "entry.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node entry.js --unhandled-rejections=strict --trace-warnings",
    "dev": "NODE_ENV=development node entry.js --unhandled-rejections=strict --trace-warnings",
    "dev:client": "cd client && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --config .eslintrc.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "color-name-list": "^8.0.0",
    "express": "^4.17.1",
    "get-image-colors": "^4.0.0",
    "google-spreadsheet": "^3.1.15",
    "human-to-cron": "^0.3.1",
    "image-type": "^4.1.0",
    "jimp": "^0.16.1",
    "leapjs-plugins": "^0.1.12",
    "lodash.throttle": "^4.1.1",
    "memory-cache": "^0.2.0",
    "ngrok": "^4.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.0.3",
    "obs-websocket-js": "^4.0.2",
    "opencv4nodejs": "^5.6.0",
    "qs": "^6.9.4",
    "runescape-text": "^1.1.10",
    "socket.io": "^4.0.0",
    "tmi.js": "^1.8.3",
    "uuid": "^8.3.1",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "chalk": "4.1.2",
    "dotenv": "10.0.0",
    "enquirer": "2.3.6",
    "eslint": "7.32.0",
    "eslint-config-preact": "1.1.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.13",
    "prettier": "2.4.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  }
}
