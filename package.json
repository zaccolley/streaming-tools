{
  "name": "streaming-tools",
  "version": "1.0.0",
  "description": "some scripts for streaming",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "prestart": "npm run build",
    "start": "GOOGLE_APPLICATION_CREDENTIALS=google-credentials.json node main.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "GOOGLE_APPLICATION_CREDENTIALS=google-credentials.json nodemon --unhandled-rejections=strict --trace-warnings main.js",
    "dev:client": "cd client && BROWSER=none npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@google-cloud/vision": "^2.3.3",
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "color-name-list": "^8.0.0",
    "express": "^4.17.1",
    "get-image-colors": "^4.0.0",
    "google-spreadsheet": "^3.1.15",
    "human-to-cron": "^0.3.1",
    "jimp": "^0.16.1",
    "memory-cache": "^0.2.0",
    "ngrok": "^4.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.0.3",
    "obs-websocket-js": "^4.0.2",
    "qs": "^6.9.4",
    "replaceall": "^0.1.6",
    "socket.io": "^4.0.0",
    "tmi.js": "^1.8.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "chalk": "4.1.1",
    "concurrently": "6.2.0",
    "dotenv": "^10.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.9",
    "prettier": "2.3.1"
  }
}
