{
  "private": true,
  "name": "streaming-tools",
  "version": "1.0.0",
  "description": "some scripts for streaming",
  "exports": "./entry.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "postinstall": "cd client && npm install",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node entry.js --unhandled-rejections=strict --trace-warnings",
    "dev": "NODE_ENV=development node entry.js --unhandled-rejections=strict --trace-warnings",
    "dev:client": "cd client && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/ --config .eslintrc.json"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "0.0.7",
    "chalk": "5.0.0",
    "chroma-js": "2.3.0",
    "color-name-list": "8.38.0",
    "dotenv": "15.0.1",
    "enquirer": "2.3.6",
    "eslint": "8.8.0",
    "eslint-config-preact": "1.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-compat": "4.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "express": "4.17.2",
    "get-image-colors": "4.0.0",
    "google-spreadsheet": "3.2.0",
    "human-to-cron": "0.3.1",
    "image-type": "4.1.0",
    "is-svg": "4.3.2",
    "jimp": "0.16.1",
    "leapjs-plugins": "0.1.12",
    "lodash.throttle": "4.1.1",
    "memory-cache": "0.2.0",
    "ngrok": "4.3.0",
    "node-cron": "3.0.0",
    "node-fetch": "3.2.0",
    "nodemon": "2.0.15",
    "nth-check": "2.0.1",
    "object-hash": "2.2.0",
    "obs-websocket-js": "4.0.3",
    "opencv4nodejs": "5.6.0",
    "prettier": "2.5.1",
    "qs": "6.10.3",
    "runescape-text": "1.1.10",
    "socket.io": "4.4.1",
    "three": "0.137.5",
    "tmi.js": "1.8.5",
    "uuid": "8.3.2",
    "ws": "8.4.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  }
}
